{
  "name": "react-fluid-swiper",
  "version": "0.3.2",
  "description": "Smooth swiper/scroller for all platforms",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbohgard/fluid",
    "directory": "packages/react-fluid-swiper"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist",
  "license": "MIT",
  "scripts": {
    "watch": "concurrently \"yarn rollup -w\" \"yarn tsc -w\"",
    "build": "yarn rollup && yarn tsc",
    "rollup": "rollup -c",
    "tsc": "tsc -d --emitDeclarationOnly --outDir dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "track-property-hook": "*"
  },
  "peerDependencies": {
    "react": "^16.8.x || ^17.x.x"
  }
}
